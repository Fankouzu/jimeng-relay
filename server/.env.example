# Upstream (Volcengine)
VOLC_ACCESSKEY=your-access-key
VOLC_SECRETKEY=your-secret-key
VOLC_REGION=cn-north-1
VOLC_HOST=visual.volcengineapi.com
VOLC_TIMEOUT=30s
UPSTREAM_MAX_CONCURRENT=1
UPSTREAM_MAX_QUEUE=100
UPSTREAM_SUBMIT_MIN_INTERVAL=0s
API_KEY_ENCRYPTION_KEY=
# Server
SERVER_PORT=8080

# Database - SQLite (default)
DATABASE_TYPE=sqlite
DATABASE_URL=./jimeng-relay.db

